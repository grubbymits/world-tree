  "compilerOptions": {
    "module": "es2015",
    "target": "ES2015",
    "rootDir": "src",
    "outDir": "dist",
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "strictPropertyInitialization": false,
    "stripInternal": true,
  },
}
